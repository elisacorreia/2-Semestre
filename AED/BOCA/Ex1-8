#include <stdio.h>

int main() {
    int N, j;
    int eh_primo;
    unsigned long long n, i;
    scanf("%d", &N);

    for (j = 0; j < N; j++) {
       
        scanf("%llu", &n);

        eh_primo = 1;

        if (n <= 1) {
            eh_primo = 0;
        } else {
            for (i = 2; i <= n / i; i++) {
                if (n % i == 0) {
                    eh_primo = 0;
                    break;
                }
            }
        }

        if (eh_primo == 1) {
            printf("primo\n");
        } else {
            printf("composto\n");
        }
    }

    return 0;
}
