#include <stdio.h>
int main(){

    int valor, cem, cinquenta, dez, um;

    scanf("%d", &valor);

   cem = valor / 100;
   valor = valor % 100;

   cinquenta = valor / 50;
   valor = valor % 50;

   dez = valor / 10;

   um = valor % 10;

    printf("NOTAS DE 100 = %d\nNOTAS DE 50 = %d\nNOTAS DE 10 = %d\nMOEDAS DE 1 = %d\n", cem, cinquenta, dez, um);
    return 0;
}
