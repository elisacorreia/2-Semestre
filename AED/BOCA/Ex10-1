#include <stdio.h>
#include <stdlib.h>

int comparar_ints(const void *a, const void *b) {
   return (*(int*)a - *(int*)b);
}

int main(){
    int i, N, x, j, l;
    int numeros[100000];

    scanf("%d", &N); 

    for (i = 0; i < N; i++){
          scanf("%d", &numeros[i]);
    }

    qsort(numeros, N, sizeof(int), comparar_ints);

    if (N == 0) {
        return 0;
    }

    int numero_campeao = numeros[0];
    int frequencia_campea = 0;

    j = 0;
    while (j < N) {
        int numero_atual = numeros[j];
        int frequencia_atual = 0;
        x = j;
        while (x < N && numeros[x] == numero_atual) {
            frequencia_atual++;
            x++;
        }
        if (frequencia_atual > frequencia_campea) {
            frequencia_campea = frequencia_atual;
            numero_campeao = numero_atual;
        }
        j = x;
   
    }
    printf("%d\n", numero_campeao);
    printf("%d\n", frequencia_campea);
    return 0;
}
