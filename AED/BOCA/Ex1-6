#include <stdio.h>

int main(){
    int i, N, K, x, menor, j, l;
    int contagem[21];

    scanf("%d %d", &N, &K); 
    
    for (l = 0; l <= K; l++){
        contagem[i] = 0;
    }

    for (i = 0; i < N; i++){
          scanf("%d", &x);
          contagem[x]++;
    }

    menor = contagem[1];

    for (j = 2; j <= K; j++){
        if (contagem[j] < menor) {
            menor = contagem[j];
        }
    }
    
    printf("%d\n", menor);
    
    return 0;
}
