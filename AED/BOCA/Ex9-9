#include <stdio.h>
int main(){

    float n;
    int num, den,i;

    scanf("%f", &n);

    den = 1000000;
    num = n * 1000000;

    if (num < den) {
        i = num;
    } else {
        i = den;
    }

    while (i >= 2) {
        if (num % i == 0 && den % i == 0) {
            num /= i;
            den /= i;
        }
        i--;
    }
    
    printf("%d/%d\n", num, den);

    return 0;
}
